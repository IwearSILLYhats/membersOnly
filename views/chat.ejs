
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/style.css" />
    <title>Create a Topic</title>
</head>
<body>
    <header>
        <%- include("components/nav") %>
      </header>
    <main>
        <h1>topic.title</h1>
        <div>
            <div>
                <h3>topic</h3>
            </div>
            <% if(locals.posts) { %>
                <ul>
                    <% locals.posts.forEach(post => { %>
                        <li>
                            <p>post.username</p>
                            <p>post.timestamp</p>
                            <p>post.content</p>
                        </li>
                    <% }) %>
                </ul>
            <% } %>
        </div>
    </main>
</body>
</html>